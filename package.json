{
  "name": "mailstring",
  "version": "0.4.1",
  "description": "Generate mailto strings for fun and profit",
  "license": "MIT",
  "repository": "iansinnott/mailstring",
  "author": {
    "name": "Ian Sinnott",
    "email": "ian@iansinnott.com",
    "url": "https://www.iansinnott.com"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "xo",
    "test": "ava --verbose",
    "build": "webpack",
    "build:example": "./build-all.sh",
    "watch": "webpack -w",
    "bump:patch": "npm version patch -m \"v%s\"",
    "bump:minor": "npm version minor -m \"v%s\"",
    "bump": "npm run bump:patch",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run test && npm run build"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "main": "dist/index.js",
  "files": [
    "src/mailTo.js"
  ],
  "keywords": [
    "mailto",
    "email",
    "react"
  ],
  "dependencies": {},
  "peerDependencies": {
    "react": ">=15.3.1"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-core": "~6.14.0",
    "babel-loader": "~6.2.5",
    "babel-preset-es2015": "~6.14.0",
    "babel-preset-react": "~6.11.1",
    "babel-preset-stage-1": "~6.13.0",
    "babel-register": "~6.14.0",
    "react": "~15.3.1",
    "webpack": "~1.13.2",
    "xo": "^0.16.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "xo/filename-case": 0,
      "babel/object-curly-spacing": 0,
      "babel/array-bracket-spacing": 0,
      "comma-dangle": [
        1,
        "always-multiline"
      ]
    }
  }
}
